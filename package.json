{
  "name": "gedcom.js",
  "version": "0.5.0",
  "description": "Convert GEDCOM Files to an JSON File or an JavaScript-Object and vice versa",
  "main": "dist/index.js",
  "scripts": {
    "demo:GEDCOM": "ts-node src/index.ts --path 'examples/simpsons.json' --convertto GEDCOM",
    "demo:JSON": "ts-node src/index.ts --path 'examples/simpsons.ged'",
    "demoFile:JSON": "ts-node src/index.ts --path 'examples/simpsons.ged' --out 'test.json'",
    "test": "nyc mocha",
    "test:watch": "mocha --watch --watch-files src, tests/**/*.ts",
    "doc": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jisco/gedcom.js.git"
  },
  "author": "Jens Hoernecke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jisco/gedcom.js/issues"
  },
  "homepage": "https://github.com/Jisco/gedcom.js#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/object-path": "^0.11.0",
    "@types/yargs": "^15.0.12",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dayjs": "^1.9.7",
    "deepdash": "^5.3.5",
    "fclone": "^1.0.11",
    "hebcal": "^2.3.2",
    "js-yaml": "^3.14.1",
    "julian-gregorian": "^1.0.0",
    "line-by-line": "^0.1.6",
    "lodash": "^4.17.20",
    "object-path": "^0.11.5",
    "yargs": "^16.2.0"
  }
}
